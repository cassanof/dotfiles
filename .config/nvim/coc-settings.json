{
  "snippets.userSnippetsDirectory": "$HOME/.config/nvim/ultisnips",
  "highlight.disableLanguages": ["txt", ""],
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
  },
  "codeLens.enable": true,
  "clangd.path": "/usr/bin/clangd",
  "clangd.semanticHighlighting": true,
  "coc.preferences.semanticTokensHighlights": false,
  "coc.preferences.formatOnSaveFiletypes": ["racket"],
  "diagnostic.checkCurrentLine": true,
  "suggest.labelMaxLength": 75,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
  "cSpell.enabledLanguageIds": [
    "git-commit",
    "latex",
    "markdown",
    "plaintext",
    "text"
  ],
  "cSpell.userWords": [
    "APFS",
    "ASLR",
    "Cassano",
    "Ciphey",
    "Combinator",
    "Excludability",
    "Exfiltration",
    "FWAE",
    "Journaling",
    "Mitigations",
    "NTFS",
    "Satisfiability",
    "Splittable",
    "Stackable",
    "excludable",
    "numericals",
    "overwritable",
    "shellcode",
    "uncongested",
    "unsanitized"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": ["/^\\s*```[\\s\\S]*?^\\s*```/gm"]
    }
  ],
  "java.format.settings.url": "/home/elleven/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.format.enabled": true,
  "java.format.onType.enabled": true,
  "java.format.comments.enabled": true,
  "java.project.referencedLibraries": [
    "lib/**/*.jar",
    "/home/elleven/.classpath/*"
  ],
  "languageserver": {
    "racket": {
      "command": "racket",
      "args": ["--lib", "racket-langserver"],
      "filetypes": ["scheme", "racket"]
    },
    "ocaml": {
      "command": "ocamllsp",
      "rootPatterns": ["dune-project"],
      "filetypes": ["ocaml"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
