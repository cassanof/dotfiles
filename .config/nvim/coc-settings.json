{
  "snippets.userSnippetsDirectory": "$HOME/.config/nvim/ultisnips",
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
  },
  "codeLens.enable": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/12.0.1/clangd_12.0.1/bin/clangd",
  "clangd.semanticHighlighting": true,
  "coc.preferences.semanticTokensHighlights": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "racket",
    "java",
    "go"
  ],
  "diagnostic.checkCurrentLine": true,
  "suggest.labelMaxLength": 75,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.checkOnSave.command": "clippy",
  "cSpell.enabledLanguageIds": [
    "git-commit",
    "latex",
    "markdown",
    "plaintext",
    "text"
  ],
  "cSpell.userWords": [
    "APFS",
    "ASLR",
    "Ciphey",
    "Combinator",
    "Exfiltration",
    "FWAE",
    "Journaling",
    "Mitigations",
    "NTFS",
    "Splittable",
    "Stackable",
    "numericals",
    "overwritable",
    "shellcode",
    "unsanitized"
  ],
  "cSpell.languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": ["/^\\s*```[\\s\\S]*?^\\s*```/gm"]
    }
  ],
  "java.format.settings.url": "/home/elleven/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "java.format.enabled": true,
  "java.format.onType.enabled": true,
  "java.format.comments.enabled": true,
  "java.project.referencedLibraries": [
    "lib/**/*.jar",
    "/home/elleven/.classpath/*"
  ],
  "languageserver": {
    "racket": {
      "command": "racket",
      "args": ["--lib", "racket-langserver"],
      "filetypes": ["scheme", "racket"]
    }
  }
}
